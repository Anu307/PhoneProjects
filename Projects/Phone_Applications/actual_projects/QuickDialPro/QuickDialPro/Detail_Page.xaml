                <TextBox x:Name="NickName10" HorizontalAlignment="Left" Height="72" Margin="216,536,0,0" TextWrapping="Wrap" Text="NickName" VerticalAlignment="Top" Width="206"/>
                <Button Content="12" HorizontalAlignment="Left" Margin="-14,592,0,-8" VerticalAlignment="Top" Width="72" Click="Button_Click"/>
                <TextBox x:Name="Number11" HorizontalAlignment="Left" Height="72" Margin="40,591,0,-7" TextWrapping="Wrap" Text="Number" VerticalAlignment="Top" Width="196"/>
                <TextBox x:Name="NickName11" HorizontalAlignment="Left" Height="72" Margin="216,591,0,-7" TextWrapping="Wrap" Text="NickName" VerticalAlignment="Top" Width="206"/>
                <Button x:Name="ButtonImage1" HorizontalAlignment="Left" Height="72" Margin="404,32,-20,0" VerticalAlignment="Top" Width="72" Click="ButtonImage_Click" Content="2"/>
                <Button x:Name="ButtonImage2" HorizontalAlignment="Left" Height="72" Margin="404,91,-20,0" VerticalAlignment="Top" Width="72" Click="ButtonImage_Click" Content="3"/>
                <Button x:Name="ButtonImage3" HorizontalAlignment="Left" Height="72" Margin="404,148,-20,0" VerticalAlignment="Top" Width="72" Click="ButtonImage_Click" Content="4"/>
                <Button x:Name="ButtonImage4" HorizontalAlignment="Left" Height="72" Margin="404,202,-20,0" VerticalAlignment="Top" Width="72" Click="ButtonImage_Click" Content="5"/>
                <Button x:Name="ButtonImage5" HorizontalAlignment="Left" Height="72" Margin="404,259,-20,0" VerticalAlignment="Top" Width="72" Click="ButtonImage_Click" Content="6"/>
                <Button x:Name="ButtonImage6" HorizontalAlignment="Left" Height="72" Margin="404,313,-20,0" VerticalAlignment="Top" Width="72" Click="ButtonImage_Click" Content="7"/>
                <Button x:Name="ButtonImage7" HorizontalAlignment="Left" Height="72" Margin="404,370,-20,0" VerticalAlignment="Top" Width="72" Click="ButtonImage_Click" Content="8"/>
                <Button x:Name="ButtonImage8" HorizontalAlignment="Left" Height="72" Margin="404,423,-20,0" VerticalAlignment="Top" Width="72" Click="ButtonImage_Click" Content="9"/>
                <Button x:Name="ButtonImage9" HorizontalAlignment="Left" Height="72" Margin="404,480,-20,0" VerticalAlignment="Top" Width="72" Click="ButtonImage_Click" Content="10"/>
                <Button x:Name="ButtonImage10" HorizontalAlignment="Left" Height="72" Margin="404,534,-20,0" VerticalAlignment="Top" Width="72" Click="ButtonImage_Click" Content="11"/>
                <Button x:Name="ButtonImage11" HorizontalAlignment="Left" Height="72" Margin="404,591,-20,-7" VerticalAlignment="Top" Width="72" Click="ButtonImage_Click" Content="12"/>


            </Grid>
        </Grid>
    </ScrollViewer>

</phone:PhoneApplicationPage>