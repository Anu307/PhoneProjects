                <TextBox x:Name="NickName5" HorizontalAlignment="Left" Height="72" Margin="216,260,0,0" TextWrapping="Wrap" Text="NickName" VerticalAlignment="Top" Width="206"/>
                <Button Content="7" HorizontalAlignment="Left" Margin="-14,313,0,0" VerticalAlignment="Top" Width="72" Click="Button_Click"/>
                <TextBox x:Name="Number6" HorizontalAlignment="Left" Height="72" Margin="40,313,0,0" TextWrapping="Wrap" Text="Number" VerticalAlignment="Top" Width="196"/>
                <TextBox x:Name="NickName6" HorizontalAlignment="Left" Height="72" Margin="216,313,0,0" TextWrapping="Wrap" Text="NickName" VerticalAlignment="Top" Width="206"/>
                <Button Content="8" HorizontalAlignment="Left" Margin="-14,370,0,0" VerticalAlignment="Top" Width="72" Click="Button_Click"/>
