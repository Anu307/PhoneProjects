                <TextBox x:Name="NickName9" HorizontalAlignment="Left" Height="72" Margin="216,481,0,0" TextWrapping="Wrap" Text="NickName" VerticalAlignment="Top" Width="206"/>
                <Button Content="11" HorizontalAlignment="Left" Margin="-14,537,0,0" VerticalAlignment="Top" Width="72" Click="Button_Click"/>
