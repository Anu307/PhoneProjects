                <Button.Background>
                    <ImageBrush Stretch="UniformToFill" ImageSource="/feature.phone.png"/>
                </Button.Background>
            </Button>
            <Button x:Name="But9" Content="Name" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Left"  HorizontalAlignment="Left" Margin="1,462,0,0" VerticalAlignment="Top" Height="156" Width="156" Click="But1_Click" FontFamily="Segoe WP Semibold" FontSize="16" >
                <Button.Background>
                    <ImageBrush Stretch="UniformToFill" ImageSource="/feature.phone.png"/>
                </Button.Background>
            </Button>
            <Button x:Name="But10" Content="Name" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Left"  HorizontalAlignment="Left" Margin="150,462,0,0" VerticalAlignment="Top" Height="156" Width="156" Click="But1_Click" FontFamily="Segoe WP Semibold" FontSize="16" >
                <Button.Background>
                    <ImageBrush Stretch="UniformToFill" ImageSource="/feature.phone.png"/>
                </Button.Background>
            </Button>
            <Button x:Name="But11" Content="Name" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Left"  HorizontalAlignment="Left" Margin="296,462,0,0" VerticalAlignment="Top" Height="156" Width="156" Click="But1_Click" FontFamily="Segoe WP Semibold" FontSize="16">
                <Button.Background>
                    <ImageBrush Stretch="UniformToFill" ImageSource="/feature.phone.png"/>
                </Button.Background>
            </Button>
            <Button x:Name="BannerButton" Content="Create your quick dialer list" HorizontalAlignment="Left" Margin="0,326,0,0" VerticalAlignment="Top" Height="298" Width="456" Click="BannerButton_Click"/>

        </Grid>
    </Grid>

</phone:PhoneApplicationPage>