            </Button>
            <Button x:Name="But4" Content="Name" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Left"  HorizontalAlignment="Left" Margin="150,172,0,0" VerticalAlignment="Top" Height="156" Width="156" Click="But1_Click" FontFamily="Segoe WP Semibold" FontSize="16" >
                <Button.Background>
